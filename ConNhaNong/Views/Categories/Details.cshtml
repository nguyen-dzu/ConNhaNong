@model ConNhaNong.Models.Category

@{
    ViewBag.Title = "Details";
}
<style>
    .img-home, .img-contact, img-product {
        display: none;
    }
</style>

<h2>Details</h2>

<div>
    <h4>Category</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Loại
        </dt>

        <dd>
            @Html.DisplayFor(model => model.category_id)
        </dd>
        <dt>
            Miêu tả
        </dt>

        <dd>
            @Html.DisplayFor(model => model.category_desc)
        </dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "Edit", new { id = Model.category_id }) |
    @Html.ActionLink("Back to List", "Index")
</p>
