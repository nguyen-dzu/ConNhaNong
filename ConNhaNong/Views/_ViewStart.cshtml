@{
    if(Session["Admin"]!=null)
    {
        Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
    }

    if (Session["User"] != null)
    {
        Layout = "~/Views/Shared/_LayoutUsers.cshtml";
    }
    if(Session["User"]==null && Session["Admin"]==null)
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
}
